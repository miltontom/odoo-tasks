<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="survey_survey_view_form"
            model="ir.ui.view">
        <field name="name">survey.survey.view_form.inherit.partner_from_survey</field>
        <field name="model">survey.survey</field>
        <field name="inherit_id"
               ref="survey.survey_survey_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//page[@name='description_done']"
                   position="after">
                <page string="Contact Relation">
                    <field name="contact_relation_id">
                        <list editable="bottom">
                            <field name="question_id" />
                            <field name="partner_field" />
                        </list>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>