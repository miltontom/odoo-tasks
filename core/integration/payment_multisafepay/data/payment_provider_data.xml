<odoo noupdate="1">
    <record id="payment_provider_multisafepay" model="payment.provider">
        <field name="name">MultiSafepay</field>
        <field name="code">multisafepay</field>
        <field name="module_id" ref="base.module_payment_multisafepay"/>
        <field name="image_128" type="base64" file="payment_multisafepay/static/description/MultiSafepay-glyph-color.svg"/>
        <field name="payment_method_ids"
               eval="[Command.set([
                         ref('payment.payment_method_card'),
                         ref('payment.payment_method_netbanking'),
                     ])]"
        />
    </record>
</odoo>