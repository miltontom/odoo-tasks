<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="vehicle_repair_labor_cost" model="ir.actions.act_window">
        <field name="name">Labor Costs</field>
        <field name="res_model">labor.cost</field>
        <field name="view_mode">list</field>
    </record>

    <record id="vehicle_repair_labor_cost_list" model="ir.ui.view">
        <field name="name">labor.cost.list</field>
        <field name="model">labor.cost</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="employee"/>
                <field name="hours_spent"/>
                <field name="hourly_cost"/>
                <field name="subtotal"/>
            </list>
        </field>
    </record>
</odoo>