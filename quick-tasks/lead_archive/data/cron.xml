<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_auto_archive_lead"
            model="ir.cron">
        <field name="name">Archive lead/oppportunity based on a threshold</field>
        <field name="model_id" ref="model_crm_lead" />
        <field name="state">code</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="code">model.action_check_time_difference()</field>
        <field name="nextcall"
               eval="(DateTime.now() + timedelta(minutes=1))" />
    </record>
</odoo>