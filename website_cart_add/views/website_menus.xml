<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="my_website_menu"
            model="website.menu">
        <field name="name">Add To Cart</field>
        <field name="url">/product_to_cart</field>
        <field name="parent_id"
               search="[('parent_id', '=', False), ('website_id', '=', 1)]" />
        <field name="website_id"
               ref="website.default_website" />
        <field name="sequence" type="int">20</field>
    </record>

</odoo>
