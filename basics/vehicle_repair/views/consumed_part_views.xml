<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="vehicle_repair_consumed_part" model="ir.actions.act_window">
        <field name="name">Labor Costs</field>
        <field name="res_model">labor.cost</field>
        <field name="view_mode">list</field>
    </record>

    <record id="vehicle_repair_consumed_part_list" model="ir.ui.view">
        <field name="name">consumed.part.list</field>
        <field name="model">consumed.part</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="product_id"/>
                <field name="quantity"/>
                <field name="unit_price"/>
                <field name="subtotal"/>
            </list>
        </field>
    </record>
</odoo>