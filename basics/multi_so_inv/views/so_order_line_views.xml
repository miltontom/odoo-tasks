<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="so_order_line_action" model="ir.actions.act_window">
        <field name="name">SO Order Line</field>
        <field name="res_model">so.order.line</field>
        <field name="view_mode">list</field>
    </record>

    <record id="so_order_line_list" model="ir.ui.view">
        <field name="name">so.order.line.list</field>
        <field name="model">so.order.line</field>
        <field name="arch" type="xml">
            <list>
                <field name="currency_id" column_invisible="1"/>
                <field name="product_id"/>
                <field name="product_uom_qty"/>
                <field name="price_unit"/>
                <field name="price_subtotal"/>
            </list>
        </field>
    </record>
</odoo>